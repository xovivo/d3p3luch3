{
  "index": "./index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "./favicon.ico",
          "./index.html",
          "./*.css",
          "./*.js",
           "./manifest.json"
        ],
        "urls": [
          "https://stackpath.bootstrapcdn.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "pais-api",
      "urls": [
        "https://ovgsoft.com/api/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "1h",
        "timeout": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
