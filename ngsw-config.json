{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.json"
        ],
        "urls": [
          "https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js",
          "https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css",
          "https://ovgsoft.com/api/**",
          "https://duckalive.com/assets/**",
          "https://sirenalive-5438a.firebaseio.com/peluches.json",
          "https://sirenalive-5438a.firebaseio.com/usuarios.json",
          "https://sirenalive-5438a.firebaseio.com/pedidos.json",
          "https://ovgsoft.com/DpelucheWeb/vistas/img/multimedia/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "dpeluche-api",
      "urls": [
        "https://ovgsoft.com/api/**"
      ],
      "cacheConfig": {
        "maxSize": 20,
        "maxAge": "1h",
        "timeout": "5s",
        "strategy": "freshness"
      }
    }
  ]
}
